{
  "name": "dewpoint-strategy-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build:server": "tsc -p tsconfig.server.json && mkdir -p dist/server/db && cp server/db/migrations.sql dist/server/db/ && echo '{\"type\":\"commonjs\"}' > dist/server/package.json",
    "build": "npm run build:server && npx vite build",
    "start": "node dist/server/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx server/db/migrate-cli.ts",
    "db:reset": "tsx server/scripts/reset-db.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "@vitejs/plugin-react": "^4.2.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.300.0",
    "openai": "^4.28.0",
    "@google/generative-ai": "^0.21.0",
    "pg": "^8.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}