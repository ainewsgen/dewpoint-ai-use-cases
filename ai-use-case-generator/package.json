{
  "name": "dewpoint-strategy-core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:server": "node --env-file=.env --import tsx/esm --watch server/index.ts",
    "build:server": "(tsc -p tsconfig.server.json || true) && mkdir -p dist/server/db && cp server/db/migrations.sql dist/server/db/",
    "build": "rm -rf dist && npx vite build && npm run build:server",
    "start": "node dist/server/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx server/db/migrate-cli.ts",
    "db:migrate:v2": "tsx server/scripts/migrate-v2.ts",
    "db:reset": "tsx server/scripts/reset-db.ts",
    "seed:icps": "tsx server/scripts/seed-icps.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.1.0",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.300.0",
    "openai": "^4.28.0",
    "pg": "^8.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.9.3",
    "vite": "5.0.12"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "tsx": "^4.21.0"
  }
}